{
  "name": "dwebp-bin",
  "version": "0.3.0",
  "description": "dwebp node.js wrapper that convert WebP into PNG",
  "bin": {
    "dwebp": "cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/1000ch/dwebp-bin.git"
  },
  "scripts": {
    "test": "xo && ava --timeout=120s",
    "postinstall": "node lib/install.js"
  },
  "bugs": {
    "url": "https://github.com/1000ch/dwebp-bin/issues"
  },
  "homepage": "https://github.com/1000ch/dwebp-bin",
  "author": {
    "name": "Shogo Sensui",
    "email": "shogosensui@gmail.com",
    "url": "https://github.com/1000ch"
  },
  "engines": {
    "node": ">=10"
  },
  "license": "MIT",
  "dependencies": {
    "bin-build": "^3.0.0",
    "bin-wrapper": "^4.1.0",
    "logalot": "^2.1.0"
  },
  "devDependencies": {
    "ava": "^3.9.0",
    "bin-check": "^4.1.0",
    "is-png": "^2.0.0",
    "tempy": "^0.5.0",
    "xo": "^0.32.0"
  },
  "files": [
    "lib",
    "vendor/source",
    "cli.js",
    "index.js"
  ],
  "keywords": [
    "image",
    "img",
    "jpg",
    "jpeg",
    "png",
    "gif",
    "webp"
  ],
  "xo": {
    "space": 2
  }
}
